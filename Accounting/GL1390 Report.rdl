    <DataSource Name="ERPDataPhocas">
      <DataSourceReference>ERPData - Phocas</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>e807cf70-e702-4489-a6a2-13fc90ec0485</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ERPDataPhocas</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @TSQL varchar(5000)

select @TSQL = 'select * from openquery(db2, ''
	select
		PJ.INVOICE 
		, PJ.VENDOR
		, V.PVVNNM as VENDOR_NAME
		, PJ.POSTING_DATE
		, PJ.AMOUNT
	from 
		(
			select
				P.VBPODT as POSTING_DATE
				, COALESCE(H.AAINVC, O.AHINVC) as INVOICE
				, COALESCE(H.AAVNNO, O.AHVNNO) as VENDOR
				, P.VBTRAM as AMOUNT
				, P.VBCONO
				, P.VBIACN
				, P.VBJRNL
				, P.VBJREF
			from 
				GLPJD P
				left join
				APHHD H
					ON P.VBCONO = H.AACONO
					and P.VBVONO = H.AAVONO
				left join
				APOHD O
					ON P.VBCONO = O.AHCONO
					and P.VBVONO = O.AHVONO
		)PJ
		inner join
		GLDET G
			on G.GTPODT = PJ.POSTING_DATE
			and G.GTCONO = PJ.VBCONO
			and G.GTIACN = PJ.VBIACN
			and G.GTJRNL = PJ.VBJRNL
			and G.GTJREF = PJ.VBJREF
		inner join
		GLMST M
			on G.GTCONO = M.GMCONO
			and G.GTIACN = M.GMIACN
		left join
		VENDR V
			on PJ.VENDOR = V.PVVNNO
	where
		PJ.POSTING_DATE between '+ RIGHT(REPLACE(convert(varchar(10), @StartDate, 120), '-', ''), 6) + ' and ' + RIGHT(REPLACE(convert(varchar(10), @EndDate, 120), '-', ''), 6) + ' 
		and M.GMGLAC like ''''05001390%''''
	order by
		PJ.INVOICE
		, PJ.VENDOR
'')'


exec(@TSQL)</CommandText>
      </Query>
