<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="ERPDataPhocas">
      <DataSourceReference>/Data Sources/ERPData - Phocas</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>a6d7e0f4-a8d4-43e5-8ced-279f19dbf6b0</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ERPDataPhocas</DataSourceName>
        <CommandText>select
	B.FORECAST as 'Planned'
	, B.ITEM_NO as 'ItemNo'
	, I.NAME as 'ItemName'
	, QTY_Avail
from 
	InvBalances B
	inner join
	Items I
		ON B.ITEM_NO = I.ITEM_NO
where
	B.WAREHOUSE_NO = '16'
	and I.CLASS = '71'
	and B.Item_NO not like 'DMG-%'
	and (
		B.FORECAST = 'Y'
		or B.FORECAST = 'N' 
		and B.QTY_SOLD_YTD &gt; 0)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Planned">
          <DataField>Planned</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ItemNo">
          <DataField>ItemNo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ItemName">
          <DataField>ItemName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="QTY_Avail">
          <DataField>QTY_Avail</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
