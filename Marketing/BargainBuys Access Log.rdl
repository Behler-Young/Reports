  <DataSources>
    <DataSource Name="BargainBuysWebapps">
      <DataSourceReference>BargainBuys - Webapps</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>29aca362-8c35-478f-998f-b10f4877527e</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>BargainBuysWebapps</DataSourceName>
        <CommandText>select 
	Username
	, count(*) as Logins
	, MAX(LogTime) as LastLogin
from 
	ActivityLogs
where
	Action = 'Login'
group by
	Username
order by 
	MAX(LogTime) DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Username">
          <DataField>Username</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Logins">
          <DataField>Logins</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="LastLogin">
          <DataField>LastLogin</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
