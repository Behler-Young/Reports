<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="ERPDataPhocas">
      <DataSourceReference>/Data Sources/ERPData - Phocas</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>b32eabf6-0328-4607-9c0b-d2b0934251dd</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ERPDataPhocas</DataSourceName>
        <CommandText>select
	A.ORDER_WRITER
	, A.WeeklyWebSales
	, C.WeeklySales
	, A.YTDWebSales
	, C.YTDSales
	, B.WeeklyWebOrders
	, D.WeeklyOrders
	, B.YTDWebOrders
	, D.YTDOrders
from
(
	select
		ORDER_WRITER
		, SUM(CASE WHEN datepart(week, GETDATE()) = datepart(week, Inv_Date_Order) THEN LINE_AMOUNT ELSE 0 END) as WeeklyWebSales
		, SUM(LINE_AMOUNT) as YTDWebSales
	from
		Sales S
	where
		Web_Order_ID &lt;&gt; ''
		and Inv_Date_Order &gt; '1/1/'+CAST(YEAR(GETDATE()) as varchar(4))
	group by
		ORDER_WRITER
) A
inner join
(
select
	ORDER_WRITER
	, SUM(CASE WHEN datepart(week, GETDATE()) = datepart(week, Inv_Date_Order) THEN 1 ELSE 0 END) as WeeklyWebOrders
	, COUNT(*) as YTDWebOrders
from
	(
		select
			DISTINCT ORDER_WRITER
			, Inv_Date_Order
			, ORDER_PARENT+ORDER_NO+ORDER_GEN as OrderID
		FROM Sales
		WHERE
			Web_Order_ID &lt;&gt; ''
			and Inv_Date_Order &gt; '1/1/'+CAST(YEAR(GETDATE()) as varchar(4))
	) S
group by
	ORDER_WRITER
) B
ON A.ORDER_WRITER = B.ORDER_WRITER
INNER JOIN
(
	select
		ORDER_WRITER
		, SUM(CASE WHEN datepart(week, GETDATE()) = datepart(week, Inv_Date_Order) THEN LINE_AMOUNT ELSE 0 END) as WeeklySales
		, SUM(LINE_AMOUNT) as YTDSales
	from
		Sales S
	where
		Inv_Date_Order &gt; '1/1/'+CAST(YEAR(GETDATE()) as varchar(4))
	group by
		ORDER_WRITER
) C
	ON A.ORDER_WRITER = C.ORDER_WRITER
inner join
(
select
	ORDER_WRITER
	, SUM(CASE WHEN datepart(week, GETDATE()) = datepart(week, Inv_Date_Order) THEN 1 ELSE 0 END) as WeeklyOrders
	, COUNT(*) as YTDOrders
from
	(
		select
			DISTINCT ORDER_WRITER
			, Inv_Date_Order
			, ORDER_PARENT+ORDER_NO+ORDER_GEN as OrderID
		FROM Sales
		WHERE
			Inv_Date_Order &gt; '1/1/'+CAST(YEAR(GETDATE()) as varchar(4))
	) S
group by
	ORDER_WRITER
) D
ON A.ORDER_WRITER = D.ORDER_WRITER
order by
	WeeklyWebSales DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
