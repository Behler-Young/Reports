  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ERPDataPhocas</DataSourceName>
        <CommandText>SELECT
	  COALESCE(MTD.PM_NAME, YTD.PM_NAME, LYTD.PM_NAME) as PM_NAME
	, COALESCE(MTD.Item_Class, YTD.Item_Class, LYTD.Item_Class) as Item_Class
	, COALESCE(MTD.DESCRIPTION, YTD.DESCRIPTION, LYTD.DESCRIPTION) as DESCRIPTION
	, COALESCE(MTD.June_Sales, 0) as June_Sales
	, COALESCE(MTD.June_GP, 0) as June_GP
	, COALESCE(YTD.YTD_Sales, 0) as YTD_Sales
	, COALESCE(YTD.YTD_GP, 0) as YTD_GP
	, COALESCE(LYTD.LYTD_Sales, 0) as LYTD_Sales
	, COALESCE(LYTD.LYTD_GP, 0) as LYTD_GP
FROM
(
	select
		  I.PM_NAME
		, I.CLASS as Item_Class
		, IC.DESCRIPTION
		, SUM(S.SALES_LINE) as June_Sales
		, SUM(S.GP_LINE) as June_GP
		, CASE WHEN SUM(S.SALES_LINE) = 0 THEN 0 ELSE SUM(S.GP_LINE) / SUM(S.SALES_LINE) END as June_GP_Perc
	from
		vw_phSales S
		inner join
		items I
			ON S.ITEM_NO = I.ITEM_NO
		inner join
		ItemClasses IC
			ON I.CLASS = IC.CLASS
	where
		S.Inv_Date_Order between '6/1/2015' and '6/30/2015'
	group by
		I.PM_NAME ,I.CLASS, IC.DESCRIPTION
) MTD
FULL JOIN
(
	select
		  I.PM_NAME
		, I.CLASS as Item_Class
		, IC.DESCRIPTION
		, SUM(S.SALES_LINE) as YTD_Sales
		, SUM(S.GP_LINE) as YTD_GP
		, CASE WHEN SUM(S.SALES_LINE) = 0 THEN 0 ELSE SUM(S.GP_LINE) / SUM(S.SALES_LINE) END as YTD_GP_Perc
	from
		vw_phSales S
		inner join
		items I
			ON S.ITEM_NO = I.ITEM_NO
		inner join
		ItemClasses IC
			ON I.CLASS = IC.CLASS
	where
		S.Inv_Date_Order between '1/1/2015' and '6/30/2015'
	group by
		I.PM_NAME ,I.CLASS, IC.DESCRIPTION
) YTD
	ON MTD.PM_NAME = YTD.PM_NAME
	and MTD.Item_Class = YTD.Item_Class
FULL JOIN
(
	select
		  I.PM_NAME
		, I.CLASS as Item_Class
		, IC.DESCRIPTION
		, SUM(S.SALES_LINE) as LYTD_Sales
		, SUM(S.GP_LINE) as LYTD_GP
		, CASE WHEN SUM(S.SALES_LINE) = 0 THEN 0 ELSE SUM(S.GP_LINE) / SUM(S.SALES_LINE) END as LYTD_GP_Perc
	from
		vw_phSales S
		inner join
		items I
			ON S.ITEM_NO = I.ITEM_NO
		inner join
		ItemClasses IC
			ON I.CLASS = IC.CLASS
	where
		S.Inv_Date_Order between '1/1/2014' and '6/30/2014'
	group by
		I.PM_NAME ,I.CLASS, IC.DESCRIPTION
) LYTD
	ON YTD.PM_NAME = LYTD.PM_NAME
	and YTD.Item_Class = LYTD.Item_Class</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
