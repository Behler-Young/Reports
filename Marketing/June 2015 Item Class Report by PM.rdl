  <DataSources>
    <DataSource Name="ERPDataPhocas">
      <DataSourceReference>/Data Sources/ERPData - Phocas</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>1e72cdc0-1c77-4056-a9b1-5b3fac89f975</rd:DataSourceID>
    </DataSource>
