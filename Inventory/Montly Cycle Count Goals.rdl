  <DataSources>
    <DataSource Name="ERPDataPhocas">
      <DataSourceReference>ERPData - Phocas</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>4afee4eb-6b09-488f-85b1-64f2c1864835</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ERPDataPhocas</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@month">
            <Value>=Parameters!month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@year">
            <Value>=Parameters!year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
	G.Warehouse
	, G.CountDate
	, SUM(ItemsPerDay) as DailyItems
from 
	CycleCountGoals G
	inner join
	PhocasMasterBY.dbo.WorkingDay D
		ON G.CountDate = D.Day
where
	MONTH(CountDate) = @month
	and YEAR(CountDate) = @year
group by
	G.Warehouse
	, G.CountDate
order by Warehouse, CountDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Warehouse">
          <DataField>Warehouse</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CountDate">
          <DataField>CountDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="DailyItems">
          <DataField>DailyItems</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
