  <DataSources>
    <DataSource Name="PhocasPhocasMasterBY">
      <DataSourceReference>Phocas - PhocasMasterBY</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>27be55bf-9a51-47d8-9bce-0903756bb75d</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>PhocasPhocasMasterBY</DataSourceName>
        <CommandText>select 
	  U.UserName
	, U.Name
	, U.GroupName
	, A.DaysActive
	, A.QueriesRan
	, A.LastActivity from
Users U
LEFT JOIN
(
	select
		U.ID,
		COUNT(*) as DaysActive,
		SUM(Count) as QueriesRan,
		MAX(A.Date) as LastActivity
	from 
		LogActivity A
		inner join
		Users U
			ON A.UserID = U.ID
	where
		A.Date &gt; DATEADD(M, -3, GETDATE())
	group by
		U.ID

) A
	ON U.ID = A.ID
where
	U.GroupName in ('Sales', 'Branch', 'Marketing', 'Hydronics', 'Product Managers')
order by
		A.LastActivity DESC, A.QueriesRan DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
